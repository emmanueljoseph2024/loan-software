{
  "initialCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "loan-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "loan-app/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/loan-app/dist/index.html"
    }
  ]
}
